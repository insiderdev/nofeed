{
    "action": {
        "type": "css-display-none",
        "selector": "a[href='/']"
    },
    "trigger": {
        "url-filter": "instagram.com"
    }
},
{
    "action": {
        "type": "css-display-none",
        "selector": "main section article, main section div div div svg"
    },
    "trigger": {
        "url-filter": "instagram.com",
        "unless-domain": ["instagram.com/"]
    }
},
{
    "action": {
        "type": "block"
    },
    "trigger": {
        "url-filter": ".*instagram.com\/graphql\/query.*stories.*"
    }
}
